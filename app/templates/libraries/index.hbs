<h2>List</h2>
<div class="row">
  {{#each model as |library|}}
    <div class="col-md-4">
      {{#library-item item=library}}
        {{#link-to 'libraries.edit' library.id class='btn btn-info btn-xs'}}
            <span class="glyphicon glyphicon-pencil"></span>
            Edit
        {{/link-to}}
        <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>
            <span class="glyphicon glyphicon-trash"></span>
            Delete
        </button>
      {{/library-item}}
    </div>
  {{/each}}
</div>
